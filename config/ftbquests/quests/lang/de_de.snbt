	quest.4E3D548E5D8D7D26.quest_desc: [
		"Vergessene Felder sind die Ebenen des &2&lUndergarden&r. \\n\\nSie haben keine Bäume, aber viele Steine, Blumen und Mobs! \\n\\nSie spawnen jedoch keine &7Vergessenen&r, ja, tut mir leid, deine Begeisterung zu dämpfen."
		"{image:atm:textures/questpics/undergarden/undergarden_field.png width:200 height:100 align:center}"
	]
	quest.4E3D548E5D8D7D26.title: "Vergessene Felder"
	quest.4E4692C5E5A3D1A9.quest_desc: ["Dies funktioniert identisch zur Bronzeversion, ist aber viel effizienter beim Übertragen von Flüssigkeiten, was zu etwa 2x so viel Wasser führt, das hindurchgepumpt wird!"]
	quest.4E4C4C962753B6AB.quest_desc: [
